{
  "apiVersion": "core.paladin.io/v1alpha1",
  "kind": "Paladin",
  "metadata": {
    "namespace": "paladin-e2e",
    "name": "node1"
  },
  "spec": {
    "config": "log:\n  level: debug",
    "database": {
      "mode": "sidecarPostgres",
      "migrationMode": "auto"
    },
    "besuNode": "node1",
    "secretBackedSigners": [
      {
        "name": "signer-1",
        "secret": "node1.keys",
        "type": "autoHDWallet"
      }
    ],
    "service": {
      "type": "NodePort",
      "ports": [
        {
          "name": "rpc-http",
          "port": 8548,
          "nodePort": 31548
        },
        {
          "name": "rpc-ws",
          "port": 8549,
          "nodePort": 31549
        }
      ]
    }
  }
}
